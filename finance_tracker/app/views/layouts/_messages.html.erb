<% if notice %>
	<div class='alert alert-success alert-dismissible'>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
		<%= notice %>
	</div>
<% end %>
<% if alert %>
	<div class='alert alert-danger alert-dismissible'>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
		<%= alert %>
	</div>
<% end %>